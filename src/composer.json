{
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php Tests",
        "coverage": "phpunit --coverage-html ./coverage",
        "lint": "phpcs",
        "fix": "phpcbf"
    },
    "require": {
        "symfony/validator": "^6.0"
    }
}
